int adc_available = 0;

void Analogue_init(void)
{

}

void Analogue_irq_callback(void)
{

}

void Analogue_process(void)
{

		if (adc_available == 1)
		{
			adc_available = 0;
			for(int itr = 0 ; itr < 4 ; itr++)
			{
				printf("%u\t", adc_tab[itr]);
			}
			printf("\r\n");
}
